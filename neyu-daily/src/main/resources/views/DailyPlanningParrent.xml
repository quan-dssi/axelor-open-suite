			<field name="dateImport" />
			<field name="dailyPlanning" grid-view="neyu-daily-planning-grid" canRemove="false" canNew="false"  edit-window="self" colSpan="12" />
		</panel>
		
	<!-- 	<panel-related field="dailyPlanning" editable="true" edit-icon="false" canNew="false" canRemove="false" canView="false"/> -->
	</form>


