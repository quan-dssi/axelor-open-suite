	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<grid name="neyu-daily-planning-parrent-grid" title="Daily Planning Parrent" model="com.axelor.neyu.db.DailyPlanningParrent">
